            "post": {
                "tags": [
                    "User"
                ],
                "summary": "Register admin",
                "description": "Register admin",
                "operationId": "registerAdmin",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "Admin object",
                        "required": true,
                        "properties": {
                            "names": {
                                "type": "string",
                                "example": "Shumbusho David"
                            },
                            "phoneNumber": {
                                "type": "string",
                                "example": "0788781096"
                            },
                            "email": {
                                "type": "string",
                                "example": "davidshumbusho10@gmail.com"
                            },
                            "password": {
                                "type": "string"
                            },
                            "nationalID": {
                                "type": "string",
                                "example": "1199999999999999"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid input"
                    }
                }
            }
        },
        "/api/v1/user/login": {
            "post": {
                "tags": [
                    "User"
                ],
                "summary": "Login user",

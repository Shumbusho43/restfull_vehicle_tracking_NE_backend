                        "required": true,
                        "properties": {
                            "email": {
                                "type": "string",
                                "example": "davidshumbusho10@gmail.com"
                            },
                            "password": {
                                "type": "string",
                                "example": "string"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid input"
                    }
                }
            }
        },
        "/api/v1/user/register/user": {
            "post": {
                "tags": [
                    "User"
                ],
                "summary": "Register user",
                "description": "Register user",
                "operationId": "registerUser",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "User object",
                        "required": true,
                        "properties": {
                            "names": {
                                "type": "string",
                                "example": "Shumbusho David"
                            },
                            "phoneNumber": {
                                "type": "string",
                                "example": "0788781096"
                            },
                            "email": {
                                "type": "string",
                                "example": "davidshumbusho10@gmail.com"
                            },
                            "password": {
                                "type": "string"
                            },
                            "nationalID": {
                                "type": "string",
                                "example": "1199999999999999"
                            },
                            "address": {
                                "type": "string",
                                "example": "Kigali"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid input"
                    }
                }
            }
        },
        "/api/v1/vehicle": {
            "post": {
                "tags": [
                    "Vehicle"
                ],
                "summary": "Add vehicle",
                "description": "Add vehicle",
                "operationId": "addVehicle",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "Vehicle object",
                        "required": true,
                        "properties": {
                            "manufactureCompany": {
                                "type": "string",
                                "example": "Toyota"
                            },
                            "manufactureYear": {
                                "type": "string",
                                "example": "2019"
                            },
                            "price": {
                                "type": "string",
                                "example": "13 million"
                            },
                            "modelName": {
                                "type": "string",
                                "example": "Toyota RAV4"
                            },
                            "owner": {
                                "type": "string",
                                "example": "645a59784c81ba5d9eb33d83"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid input"
                    }
                }
            }
        },
        "/api/v1/vehicle/{page}/{perPage}": {
            "get": {
                "tags": [
                    "Vehicle"
                ],
                "summary": "Get all vehicles",
                "description": "Get all vehicles",
                "operationId": "getAllVehicles",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "path",
                        "name": "page",
                        "description": "page number",
                        "required": true,
                        "type": "integer",
                        "format": "int32"
                    },
                    {
                        "in": "path",
                        "name": "perPage",
                        "description": "number of items per page",
                        "required": true,
                        "type": "integer",
                        "format": "int32"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid input"
                    }
                }
            }
        }
